  <script type="text/javascript">
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {

      var data = google.visualization.arrayToDataTable([
        ['Task', 'parameter1'],
        ['Doctors',     100],
        ['Patients',      250],
       
      ]);

      var cata = google.visualization.arrayToDataTable([
        ['Doctor', 'Number of doctors'],
        ['Assigned Doctors',     50],
        ['Unassigned Doctors',      50],
       
      ]);

      var ata = google.visualization.arrayToDataTable([
        ['Patients', 'Number of patients'],
        ['Active Patients',     200],
        ['Discharged Patients',      50],
       
      ]);



      var options = {
        title: 'No of Doctor vs No of Patients'
      };

      var options1 = {
        title: 'Assigned Doctors vs Unassigned Doctors'
      };

      var options2 = {
        title: 'Active Patients vs Discharged Patients'
      };

      var chart = new google.visualization.PieChart(document.getElementById('piechart1'));
      var chart1 = new google.visualization.PieChart(document.getElementById('piechart2'));
      var chart2 = new google.visualization.PieChart(document.getElementById('piechart3'));


      chart.draw(data, options);
      chart1.draw(cata, options1);
      chart2.draw(ata, options2);
    }
  </script>
