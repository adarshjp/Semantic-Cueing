<div class="sidebar close">
    <div class="logo-details">
      <i class='bx bx-menu' ></i>
      <span class="logo_name">Menu</span>
    </div>
    <ul class="nav-links">
      <li>
        <a href="/home/admin">
          <div onClick="showHome()">
            <i class='bx bx-grid-alt' ></i>
            <span class="link_name" >Home</span>
          </div>
        </a>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="/home/admin">Home</a></li>
        </ul>
      </li>
      <li>
        <div class="iocn-link" onClick="showDoctors()">
          <div>
            <i class='bx bx-collection' ></i>
            <span class="link_name" >Doctor</span>
          </div>
        </div>
        <ul class="sub-menu">
          <li><a class="link_name" href="#">Doctor</a></li>
        </ul>
      </li>
      <li>
        <div class="iocn-link" onClick="showPatients()">
          <div>
            <i class='bx bx-book-alt' ></i>
            <span class="link_name" >Patients</span>
          </div>
        </div>
        <ul class="sub-menu">
          <li><a class="link_name" href="#">Patients</a></li>
        </ul>
      </li>
      <li>
        <a href="/addquestion">
          <div class="iocn-link" onClick="showAddQuestion()">
            <div>
              <i class="fas fa-question"></i>
              <span class="link_name" >Add Question</span>
            </div>
          </div>
        </a>
          <ul class="sub-menu">
            <li><a class="link_name" href="/addquestion">Add Question</a></li>
          </ul>
      </li>
      <li>
        <div>
          <a href="/logout"><i class='bx bx-log-out' ></i></a>
          <span class="link_name" id="logout" ><a href="/logout" style="color: white">Logout</span></a>
       </div>
        <ul class="sub-menu blank">
          <li><a class="link_name" href="/logout">Logout</a></li>
        </ul>
      </li>
      <li>
        <div class="profile-details" onClick="showProfile()">
          <div class="profile-content" >
            <img src="/images/profile.jpg" alt="profileImg">
          </div>
          <div class="name-job">
            <div class="profile_name" ><%=user.name%></div>
          </div>
        </div>
      </li>
    </ul>
</div>